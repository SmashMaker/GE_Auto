{
  "manifest_version": 3,
  "name": "QE_Auto",
  "version": "1.0",
  "description": "Execute some tasks automatically on the page ",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
    }
  }
}
